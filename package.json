{
  "name": "no-trackers",
  "version": "1.0.0",
  "description": "Cross browser library to disable/enable trackers. Great for following CCPA guidelines",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require @babel/register test/unit.js",
    "build": "node_modules/.bin/browserify -g uglifyify index.js -o dist/no-trackers.min.js -t [ babelify --presets [ @babel/preset-env ] ]",
    "eslint": "eslint index.js test/*"
  },
  "keywords": [
    "tracker",
    "CCPA",
    "cookielaw",
    "javascript",
    "fetch",
    "xhr",
    "browser"
  ],
  "author": "Maxime Helen",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.1",
    "mockery": "^2.1.0",
    "should": "^13.2.3",
    "uglifyify": "^5.0.2"
  }
}
